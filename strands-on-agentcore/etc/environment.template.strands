# aws configuration
PROFILE=your-aws-cli-profile
REGION=us-east-1
BUCKET=your-s3-bucket-for-deployment
ACCOUNTID=your-aws-account-id

# infrastructure configuration
P_REPOSITORY_NAME=agentcore/hello
P_VPC_ID=your-vpc-id
P_CLIENT_INGRESS_CIDR=your-allowed-ingress-cidr
INFRASTRUCTURE_STACK=agentcore-infrastructure
INFRASTRUCTURE_TEMPLATE=iac/infrastructure.yaml
INFRASTRUCTURE_OUTPUT=iac/infrastructure_output.yaml
INFRASTRUCTURE_PARAMS="ParameterKey=repositoryName,ParameterValue=${P_REPOSITORY_NAME} ParameterKey=vpcId,ParameterValue=${P_VPC_ID} ParameterKey=clientIngressCidr,ParameterValue=${P_CLIENT_INGRESS_CIDR}"
O_ECR_REPO=output-repo-name
O_ECR_REPO_ARN=output-repo-arn
O_ECR_REPO_URI=output-repo-uri
O_AGENT_SGROUP_ID=output-security-group-id
O_AGENT_ROLE_ARN=output-role-arn

# container configuration
C_DOCKERFILE=dockerfile.strands
C_REPO_BASE=${ACCOUNTID}.dkr.ecr.${REGION}.amazonaws.com
C_REPO_IMAGE=${O_ECR_REPO}
C_VERSION=1
C_TAG=${C_REPO_IMAGE}:${C_VERSION}
C_REPO_URI=${C_REPO_BASE}/${C_REPO_IMAGE}:${C_VERSION}

# agentcore configuration
P_AGENT_NAME=your-agent-name
P_CONTAINER_URI=${C_REPO_URI}
P_PROTOCOL_CONFIGURATION=HTTP
P_NETWORK_CONFIGURATION=PUBLIC
P_SECURITY_GROUPS=${O_AGENT_SGROUP_ID}
P_SUBNETIDS_PRIVATE=your-commas-separated-list-of-private-subnet-ids
RUNTIME_STACK=agentcore-hello-runtime
RUNTIME_TEMPLATE=iac/template.yaml
RUNTIME_OUTPUT=iac/template_output.yaml
RUNTIME_PARAMS="ParameterKey=agentName,ParameterValue=${P_AGENT_NAME} ParameterKey=containerUri,ParameterValue=${P_CONTAINER_URI} ParameterKey=roleArn,ParameterValue=${O_AGENT_ROLE_ARN} ParameterKey=networkConfiguration,ParameterValue=${P_NETWORK_CONFIGURATION} ParameterKey=securityGroups,ParameterValue=${P_SECURITY_GROUPS} ParameterKey=subnets,ParameterValue=${P_SUBNETIDS_PRIVATE} ParameterKey=protocolConfiguration,ParameterValue=${P_PROTOCOL_CONFIGURATION}"
O_AGENT_ID=output-agent-id
O_AGENT_ARN=arn:aws:bedrock-agentcore:${REGION}:${ACCOUNTID}:runtime/${O_AGENT_ID}
O_AGENT_VERSION=1
O_AGENT_QUALIFIER=DEFAULT

# agent pre-configuration
EXECUTION_ROLE=${O_AGENT_ROLE_ARN}
AGENT_NAME=strands_example
SERVER_PROTOCOL=HTTP
ENV_VARS={"AGENT_NAME": "${AGENT_NAME}"}

# agent deployed configuration
O_AGENT_ARN=output-agent-runtime-arn
O_AGENT_ID=output-agent-runtime-id
O_AGENT_VERSION=DEFAULT

# agent invocation configuration
P_PROMPT='What is VO2 max? What is the best way to improve it?'

# otel configuration
export OTEL_METRICS_EXPORTER=none
export OTEL_TRACES_EXPORTER=none
export OTEL_LOGS_EXPORTER=none
export OTEL_RESOURCE_DETECTORS=none

# platform configuration: linux/amd64 | linux/arm64
PLATFORM_DEPLOY=linux/arm64
PLATFORM_LOCAL=linux/arm64
HOST_PORT=8080
CONTAINER_PORT=8080